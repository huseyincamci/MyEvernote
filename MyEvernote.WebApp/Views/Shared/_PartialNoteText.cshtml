@model MyEvernote.Entities.Note

<div class="row">
    <div class="col-md-12">
        <h3>@Model.Title</h3>
    </div>
    <div class="col-md-12">
        <span class="fa fa-user"></span> @Model.Owner.Username
        <span class="fa fa-calendar-times-o"></span> @Model.ModifiedOn
        <hr />
    </div>
    <div class="col-md-12">
        <p>
            @Model.Text
        </p>
    </div>
</div>
